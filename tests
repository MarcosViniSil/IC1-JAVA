
curl -X 'POST' \
  'http://localhost:8080/api/auth/register' \
  -H 'accept: */*' \
  -H 'Content-Type: application/json' \
  -d '{
  "id": 1,
  "nome": "string",
  "email": "email@mail.com",
  "senha": "12345678",
  "role": "ADMIN",
  "dtAlteracao": "2025-10-05T22:23:14.931Z",
  "dtCriacao": "2025-10-05T22:23:14.931Z",
  "primeiroNome": "nome"
}'


curl -X 'POST' \
  'http://localhost:8080/api/auth/login' \
  -H 'accept: */*' \
  -H 'Content-Type: application/json' \
  -d '{
  "email": "email@mail.com",
  "senha": "12345678"
}'


curl -H "Authorization: Bearer token_here " http://localhost:8080/api/v1/admin/teste


curl  http://localhost:8080/api/v1/admin/teste
